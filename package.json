{
  "name": "pn-finalists",
  "version": "0.2.3",
  "description": "A drop-in component to display an interactive block of PN finalists.",
  "main": "app/index.js",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --port 7502 --content-base=./public --history-api-fallback --devtool source-map",
    "build": "webpack --progress --colors --optimize-minimize --optimize-dedupe",
    "deploy": "NODE_ENV=production npm run build && git add dist/* && git commit -m 'build(deploy)' && git push heroku develop:master",
    "preversion": "NODE_ENV=production npm run build && git add -A",
    "postversion": "git push && git push --tags",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PrecisionNutrition/pn-finalists"
  },
  "author": "Jason Lengstorf (@jlengstorf) <jason@lengstorf.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PrecisionNutrition/pn-finalists/issues"
  },
  "dependencies": {
    "flux": "^2.1.1",
    "immutable": "^3.7.6",
    "react": "^0.14.7",
    "react-addons-css-transition-group": "^0.14.7",
    "react-dom": "^0.14.0",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^5.7.0",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.23.1",
    "cssnano": "^3.4.0",
    "json-loader": "^0.5.4",
    "postcss-cssnext": "^2.4.0",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.8.0",
    "postcss-mixins": "^4.0.0",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^1.2.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.10.1",
    "webpack-dev-server": "^1.10.1"
  }
}
